{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|html))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}